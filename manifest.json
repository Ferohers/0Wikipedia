{
  "manifest_version": 2,
  "short_name": "0Wikipedia",
  "name": "0Wikipedia",
  "description": "This extension makes you to access 0Wikipedia mirror directly without putting 0 to the head manually.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "icons/0Wikipedia_icon19.png"
  },

  "icons": { 
	"16": "icons/0Wikipedia_icon16.png",
	"38": "icons/0Wikipedia_icon38.png",
        "48": "icons/0Wikipedia_icon48.png",
        "128": "icons/0Wikipedia_icon128.png"
	},
	  
	"permissions":["*://*.wikipedia.org/*", "webRequest", "webRequestBlocking", "storage"]
}

